{
    "name": "zonuexe/baguette",
    "description": "Simple and High modularity web framework for PHP.",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "USAMI Kenta",
            "email": "tadsan@zonu.me"
        }
    ],
    "keywords": [
        "microframework",
        "webapp"
    ],
    "require": {
        "psr/log": "~1.0.0"
    },
    "require-dev": {
         "phpunit/phpunit": "~4.3.5"
        ,"theseer/phpdox":  "~0.7.0"
        ,"phploc/phploc":   "~2.0.6"
        ,"phpmd/phpmd":     "@stable"
        ,"satooshi/php-coveralls": "dev-master"
        ,"twig/twig":       "~1.18.0"
        ,"zonuexe/simple-routing": "~0.2.0"
        ,"zonuexe/http-accept-language": "~0.4.1"
    },
    "autoload": {
        "psr-4": {
            "Baguette\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Baguette\\": "tests/"
        }
    },
    "suggest": {
        "twig/twig": "template engine"
       ,"monolog/monolog": "logger"
       ,"zonuexe/simple-routing": "simple router for web app"
       ,"zonuexe/http-accept-language": "HTTP Accept-Language header parser"
    }
}
